 <script>
	//불자료형 - 비교 연산자를 활용하여 참과 거짓의 구별(39page)----------------------------------------------------
	alert(52>273);
	alert(52<273);

	//불 자료형끼리 비교(41페이지)-----------------------------------------------------------------------------
	alert(true > false);
	

	//불 표현식과 조건문(41페이지)------------------------------------------------------------------------------
	if(273 <52){
		alert('273은 52보다 작습니다.')
	};
	
	if(273>52){
		alert('273은 52보다 큽니다.')
	}


	//논리 부정 연산자(42페이지)------------------------------------------------------------------------------
	alert(!true);
	alert(!false);


	//비교 연산자만 사용(43페이지)(#엉뚱한 결과가 나오는 NG 사례에 해당된다.)------------------------------------------
	alert(30>20>10);//왼쪽부터 참거짓 연산이 시작된다. 
					//flase가 나오는 이유는 30>20을 논리연산하면 ture가 나오는데
					//true는 1이라는 숫자값을 갖기 때문.
					//true>10 = 1>10과 같은 것이 되버리며 결국 flase가 된다.


	//비교 연산자와 논리 연산자를 함께 사용(44페이지)
	//논리연산자를 함께 쓰면
	//비교 연산자만 사용시의 엉뚱한 결과가 나오지 않도록 해준다.
	alert(30>20 && 20>10);




 </script>
