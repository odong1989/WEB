import global.sesoc.sample.vo.Notice;

public interface NoticeMapper {
	
	//Person 객체의 값을 DB에 저장
	public int noticeInsert(Notice notice);
	
	public ArrayList<Notice> noticeSelectList(RowBounds rb );
